{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "comma-dangle": "off",
    "no-console": "off",
    "arrow-parens": "off"
  }
}
